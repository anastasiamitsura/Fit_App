<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homebt">

    <fragment
        android:id="@+id/homebt"
        android:name="com.example.fit_app.HomeSc"
        android:label="Корзина"
        tools:layout="@layout/fragment_home_sc" >

        <action
            android:id="@+id/action_homeSc_to_profileFr"
            app:destination="@id/profilebt" />
        <action
            android:id="@+id/action_homeSc_to_settingFr"
            app:destination="@id/settingbt" />
    </fragment>
    <fragment
        android:id="@+id/settingbt"
        android:name="com.example.fit_app.SettingFr"
        android:label="Настройки"
        tools:layout="@layout/fragment_setting" >

        <action
            android:id="@+id/action_settingFr_to_profileFr2"
            app:destination="@id/profilebt" />
        <action
            android:id="@+id/action_settingFr_to_homeSc2"
            app:destination="@id/homebt" />
    </fragment>
    <fragment
        android:id="@+id/profilebt"
        android:name="com.example.fit_app.ProfileFr"
        android:label="Профиль"
        tools:layout="@layout/fragment_profile" >

        <action
            android:id="@+id/action_profileFr_to_settingFr2"
            app:destination="@id/settingbt" />
        <action
            android:id="@+id/action_profileFr_to_homeSc2"
            app:destination="@id/homebt" />
    </fragment>
</navigation>